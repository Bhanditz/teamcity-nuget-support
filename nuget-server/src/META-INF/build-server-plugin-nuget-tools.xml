<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd"
       default-autowire="constructor">
  <bean class="jetbrains.buildServer.nuget.server.toolRegistry.AvailableToolsStateHolder"/>
  <bean class="jetbrains.buildServer.nuget.server.toolRegistry.impl.impl.AvailableOnPackagesNugetOrg"/>
  <bean class="jetbrains.buildServer.nuget.server.toolRegistry.impl.impl.AvailableOnDistNugetOrg"/>
  <bean class="jetbrains.buildServer.nuget.server.toolRegistry.impl.NuGetToolsSettings"/>
  <bean class="jetbrains.buildServer.nuget.server.toolRegistry.impl.impl.NuGetToolManagerImpl"/>
  <bean class="jetbrains.buildServer.nuget.server.toolRegistry.impl.impl.NuGetToolDownloaderImpl"/>
  <bean class="jetbrains.buildServer.nuget.server.toolRegistry.NuGetToolUsageStatisticsProvider"/>

  <bean class="jetbrains.buildServer.nuget.server.toolRegistry.NuGetServerToolProvider"/>
</beans>